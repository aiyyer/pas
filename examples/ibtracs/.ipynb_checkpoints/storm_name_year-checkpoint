   "source": [
    "# select basin\n",
    "basinWant = str.encode(\"NA\")\n",
    "years = pd.to_datetime(ds.time[:,0].values).year\n",
    "\n",
    "# get the indices \n",
    "inds = np.where( (ds.basin[:,0] == basinWant) & (years>=yearStart) & (years<=yearEnd))[0]\n",
    "\n",
    "#\n",
    "stormYears = pd.to_datetime(ds.time[inds,0].values).year\n",
    "stormNames = ds.name[inds].values"
   ]
  },
